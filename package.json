{
  "name": "sample-teacher-server",
  "version": "0.0.0",
  "description": "Scaffolding for a serverless typescript dotenv project",
  "main": "index.js",
  "scripts": {
    "env": "npx @siliconjungles/rn-scripts replace_files path=env/${ENV}/.cfg.${ENV}",
    "_start": "ts-node index.ts",
    "start": "ENV=${ENV:-local} yarn env && yarn _start",
    "test": "jest --setupFiles dotenv/config",
    "test_coverage": "yarn test --collectCoverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint_fix": "yarn lint --fix --plugin unused-imports --rule 'unused-imports/no-unused-imports-ts: warn'",
    "compile": "tsc --noEmit && yarn lint"
  },
  "author": "aelesia",
  "license": "MIT",
  "devDependencies": {
    "@siliconjungles/rn-scripts": "^0.0.0-42",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.12",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^9.0.0",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@aelesia/commons": "^0.5.0-next.40",
    "express": "^4.17.1",
    "dotenv": "^8.2.0"
  },
  "repository": ""
}
