{
  "name": "sample-teacher-server",
  "version": "0.0.0",
  "description": "NodeJS with TypeORM and Koa",
  "main": "index.js",
  "scripts": {
    "env": "npx @siliconjungles/rn-scripts replace_files path=env/${ENV}/.cfg.${ENV}",
    "_start": "PORT=${PORT:-3000} ts-node --log-error index.ts",
    "start": "ENV=${ENV:-local} yarn env && yarn _start",
    "test": "ENV=${ENV:-test} yarn env && jest --setupFiles dotenv/config",
    "test_coverage": "yarn test --collectCoverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint_fix": "yarn lint --fix --plugin unused-imports --rule 'unused-imports/no-unused-imports-ts: warn'",
    "compile": "tsc --noEmit && yarn lint",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db": "ENV=${ENV} yarn env && ts-node db/db.ts",
    "db:seed": "ENV=${ENV} yarn env && ts-node ./db/seed.ts",
    "db:generate": "yarn db && yarn typeorm migration:generate",
    "db:migrate": "ENV=${ENV} yarn env && yarn typeorm migration:run",
    "db:rollback": "ENV=${ENV} yarn env && yarn typeorm migration:revert"
  },
  "author": "aelesia",
  "license": "MIT",
  "devDependencies": {
    "@siliconjungles/rn-scripts": "^0.0.0-42",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/koa": "^2.11.4",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/lodash": "^4.14.161",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-unused-imports": "^0.1.3",
    "faker": "^4.1.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "sqlite3": "^4.0.3",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "3.9.7",
    "typeorm-test-transactions": "^1.13.0"
  },
  "dependencies": {
    "@aelesia/commons": "^0.5.0",
    "dotenv": "^8.2.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^9.4.0",
    "lodash": "^4.17.20",
    "mysql": "^2.18.1",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.28",
    "typeorm-naming-strategies": "^2.0.0"
  },
  "repository": ""
}
