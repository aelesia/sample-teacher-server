{
  "name": "sample-teacher-server",
  "version": "0.0.0",
  "description": "Scaffolding for a serverless typescript dotenv project",
  "main": "index.js",
  "scripts": {
    "env": "npx @siliconjungles/rn-scripts replace_files path=env/${ENV}/.cfg.${ENV}",
    "_start": "PORT=${PORT:-3000} ts-node index.ts",
    "start": "ENV=${ENV:-local} yarn env && yarn _start",
    "test": "jest --setupFiles dotenv/config",
    "test_coverage": "yarn test --collectCoverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint_fix": "yarn lint --fix --plugin unused-imports --rule 'unused-imports/no-unused-imports-ts: warn'",
    "compile": "tsc --noEmit && yarn lint",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db": "ENV=${ENV:-local} yarn env && ts-node ./src/db.ts",
    "db:generate": "yarn db && yarn typeorm migration:generate",
    "db:migrate": "yarn typeorm migration:run"
  },
  "author": "aelesia",
  "license": "MIT",
  "devDependencies": {
    "@siliconjungles/rn-scripts": "^0.0.0-42",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "faker": "^4.1.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@aelesia/commons": "^0.5.0-next.40",
    "@types/koa": "^2.11.4",
    "@types/koa-router": "^7.4.1",
    "dotenv": "^8.2.0",
    "koa": "^2.13.0",
    "koa-router": "^9.4.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^4.0.3",
    "typeorm": "0.2.28"
  },
  "repository": ""
}
